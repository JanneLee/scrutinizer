    "license": "Apache2",

    "repositories": [
        {"type": "git", "url": "git://github.com/schmittjoh/PHP-Parser"},
        {"type": "git", "url": "git://github.com/schmittjoh/Process"}
    ],

    "require": {
        "php": ">=5.4",
        "scrutinizer/utils": "dev-master",
        "symfony/config": "2.*",
