<?php

namespace Foo;

class FooTest extends \PHPUnit_Framework_TestCase
{
    public function testGetBar()
    {
        $foo = new Foo($bar = new Bar('Foo'));
        $this->assertSame($bar, $foo->getBar());
    }
}

