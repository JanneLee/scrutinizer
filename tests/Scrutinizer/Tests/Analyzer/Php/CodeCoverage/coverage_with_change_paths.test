<?php

namespace Foo;

class Foo
{
    private $bar;

    public function __construct(Bar $bar)
    {
        $this->bar = $bar;
    }

    public function getBar()
    {
        return $this->bar;
    }

    public function getBarName()
    {
        return $this->bar->getName();
    }
}

